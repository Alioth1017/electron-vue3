{
  "name": "Electron_Template",
  "productName": "Electron_Template",
  "version": "0.1.0",
  "description": "Electron_Template",
  "main": "dist/main.bundle.js",
  "private": true,
  "author": "youliso@Hotmail.com",
  "license": "MIT",
  "scripts": {
    "dev:sever": "webpack-dev-server --config ./resources/script/webpack.server.js",
    "dev:start": "cross-env NODE_ENV=development webpack --config ./resources/script/webpack.main.config.js && electron dist/main.bundle.js",
    "prod": "cross-env NODE_ENV=production node resources/script/webpack.config.js",
    "start": "electron dist/main.bundle.js",
    "build": "node resources/script/build.js && electron-builder -c resources/script/build.json"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.33",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.0.1",
    "externals-dependencies": "^1.0.4",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "style-loader": "^1.2.1",
    "svelte": "^3.25.1",
    "svelte-loader": "^2.13.6",
    "svelte-spa-router": "^3.0.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "electron-updater": "^4.3.1",
    "socket.io-client": "^2.3.0"
  }
}
