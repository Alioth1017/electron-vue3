{
  "name": "electrontemplate",
  "productName": "electrontemplate",
  "version": "0.0.1",
  "description": "electron template",
  "main": "dist/main.bundle.js",
  "author": {
    "name": "youliso",
    "email": "youliso@Hotmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "ts-node --project build/script/tsconfig.json build/script/dev.ts",
    "rebuild": "electron-rebuild",
    "build:win": "ts-node build/script/build.ts && electron-builder -c build/cfg/build.json --win",
    "build:linux": "ts-node build/script/build.ts && electron-builder -c build/cfg/build.json --linux",
    "build:mac": "ts-node build/script/build.ts && electron-builder -c build/cfg/build.json --mac"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.8",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.6",
    "builtin-modules": "^3.2.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.4",
    "esbuild": "^0.8.54",
    "rollup": "^2.40.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.4",
    "vue-router": "^4.0.4"
  },
  "dependencies": {
    "electron-updater": "^4.3.5",
    "node-abort-controller": "^1.1.0",
    "node-fetch": "^2.6.1",
    "socket.io-client": "^3.1.2",
    "vue": "^3.0.6"
  }
}
